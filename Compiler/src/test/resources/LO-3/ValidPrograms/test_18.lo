class circle(int radius;) {

  void circle(int data){
    {
      radius = data;
    }
  }

   int area() {
       {
        return (3 * (radius * radius));
      }
   }
}

class Main() {
  int main() {
    circle c;
       {
        c = new circle (4);
        return c.area();
       }
  }
}