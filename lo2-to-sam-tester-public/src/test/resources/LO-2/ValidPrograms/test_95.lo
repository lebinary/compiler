int main() {
    int a;
    {
     a= fibo(17);
     return a;
    }
}

int fibo(int a) {
    int b;
    {
     if ((a=1))
     {
       b = 1;
     }
     else
     {
       if((a=2))
       {
         b = 1;
       }
       else
       {
         b = (fibo((a-1)) + fibo((a-2)));
       }
       
      }
      return b;
    }
}